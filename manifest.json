{
  "manifest_version": 3,
  "name": "official-tube",
  "version": "1.1.1",
  "description": "Filter only official videos",
  "icons": {
    "16": "assets/icon16.png",
    "32": "assets/icon32.png",
    "64": "assets/icon64.png",
    "128": "assets/icon128.png",
    "256": "assets/icon256.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "assets/*"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "permissions": [
    "webNavigation",
    "storage",
    "scripting",
    "background"
  ],
  "background": {
    "service_worker": "workers/background.js",
    "type": "module"
  },
  "host_permissions": [
    "https://www.youtube.com/results?*"
  ]
}